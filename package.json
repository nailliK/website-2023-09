  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@ts-stack/markdown": "^1.5.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-css-order": "^2.0.0",
    "prettier-plugin-sort-json": "^3.1.0",
    "raw-loader": "^4.0.2",
    "standard": "^17.1.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "lint": "standard src/**/*.ts --fix",
    "format": "prettier ./src --write",
    "build": "yarn run clean && yarn run compile && yarn run copy",
    "clean": "rm -rf dist",
    "compile": "NODE_ENV=production tsc --lib es6 webpack.config.ts --esModuleInterop && webpack --config webpack.config.js",
    "copy": "cp src/404.html dist/404.html && cp src/favicon.svg dist/favicon.svg && cp src/favicon.png dist/favicon.png",
    "dev": "tsc --lib es6 webpack.config.ts --esModuleInterop && webpack serve --config webpack.config.js",
    "serve": "http-server dist/ -p 8080"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  }
}
