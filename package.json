	"license": "MIT",
	"private": false,
	"devDependencies": {
		"@babel/core": "^7.22.20",
		"@babel/preset-env": "^7.22.20",
		"@babel/preset-typescript": "^7.22.15",
		"@linthtml/linthtml": "^0.9.5",
		"css-loader": "^6.8.1",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.3",
		"prettier": "^3.0.3",
		"standard": "^17.1.0",
		"style-loader": "^3.3.3",
		"stylelint": "^15.10.3",
		"stylelint-config-standard": "^34.0.0",
		"ts-loader": "^9.4.4",
		"tsc": "^2.0.4",
		"typescript": "^5.2.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"scripts": {
		"lint": "yarn run lint:css && yarn run lint:html && yarn run lint:ts",
		"lint:css": "stylelint src/**/*.css --fix",
		"lint:html": "linthtml src/**/*.html --fix",
		"lint:ts": "standard src/**/*.ts --fix",
		"build": "NODE_ENV=production tsc --lib es6 webpack.config.ts --esModuleInterop && webpack --config webpack.config.js && cp src/404.html dist/404.html",
		"dev": "tsc --lib es6 webpack.config.ts --esModuleInterop && webpack serve --config webpack.config.js",
		"serve": "yarn build && http-server dist/ -p 8080"
	},
	"dependencies": {
		"@ts-stack/markdown": "^1.5.0",
		"babel-loader": "^9.1.3",
		"http-server": "^14.1.1",
		"raw-loader": "^4.0.2"
	}
}
