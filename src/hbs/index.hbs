  <section id="experience" >
    <h2 >Experience</h2 >

    {{#each resume.experience}}
      <article id="{{slugify company}}" >
        <h3 >{{company}}</h3 >
        <p >
          <strong>{{position}}</strong>, {{formatDate duration.startDate}} - {{formatDate duration.endDate}}
        </p >

        {{#if description}}
        <p >{{description}}</p >
        {{/if}}

        {{#if responsibilities}}
        <ul >
          {{#each responsibilities}}
            <li >
             {{this}}
            </li >
          {{/each}}
        </ul >
        {{/if}}

        {{#if technologies}}
          <ul class="inline" >
            {{#each skills}}
              <li >
                {{this}}
              </li >
            {{/each}}
            {{#each technologies}}
              <li >
                {{this}}
              </li >
            {{/each}}
          </ul >
        {{/if}}
      </article >
    {{/each}}
  </section >

  <section id="education" >
    <h2 >Education</h2 >
    {{#each resume.education}}
      <article id="{{slugify school}}" >
        <h3 >{{school}}</h3 >
        <p >
          <strong>{{degree}}</strong>
        </p >
      </article >
    {{/each}}
  </section >
</main >
</body >
</html >
